{
  "app": {
    "name": "react-starter",
    "version": "0.0.1",
    "build": "1"
  },
  "private": true,
  "main": "main.js",
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src/; exit 0",
    "lint:css": "stylelint ./src/**/*.css",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm run build:prod",
    "build:dev": "NODE_ENV=development node -r babel-register tools/build.js",
    "build:prod": "NODE_ENV=production node -r babel-register tools/build.js",
    "clean": "rm -rf build && rm -rf release",
    "start": "npm run start:web:dev --",
    "start:web:dev": "NODE_ENV=development node -r babel-register tools/web/server",
    "start:web:prod": "NODE_ENV=production node -r babel-register tools/web/server",
    "start:app": "npm run start:app:dev --",
    "start:app:dev": "electron -r babel-register ./tools/electron/main.js",
    "start:server": "npm run start:server:dev --",
    "start:server:dev": "NODE_ENV=development node -r babel-register tools/electron/server.js --target app",
    "package": "NODE_ENV=production node -r babel-register tools/electron/package.js",
    "check": "npm outdated --depth=0",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "apollo-client": "^0.8.7",
    "fastclick": "1.0.6",
    "graphql-tag": "^1.2.4",
    "history": "4.5.1",
    "immutable": "3.8.1",
    "medium-draft": "^0.4.1",
    "react": "^15.4.2",
    "react-apollo": "^0.11.2",
    "react-dom": "^15.4.2",
    "react-remarkable": "^1.1.1",
    "react-router": "3.0.1",
    "remarkable": "^1.7.1",
    "whatwg-fetch": "2.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-webpack": "6.4.3",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.20.0",
    "browser-sync": "2.18.6",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.26.1",
    "electron": "1.4.14",
    "electron-packager": "8.5.0",
    "enzyme": "^2.7.1",
    "eslint": "3.13.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "express": "4.14.0",
    "file-loader": "0.9.0",
    "flow-bin": "^0.37.4",
    "flow-typed": "^2.0.0",
    "html-loader": "0.4.4",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "postcss-cssnext": "2.9.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "^1.2.2",
    "postcss-reporter": "3.0.0",
    "postcss-variables-loader": "^2.1.3",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "1.3.1",
    "react-test-renderer": "15.4.2",
    "redux-mock-store": "^1.2.1",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "stylelint": "7.7.1",
    "stylelint-config-standard": "15.0.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/tests/mocks/fileMock.js",
      "^.+\\.(css|less)$": "<rootDir>/tools/tests/mocks/styleMock.js"
    }
  }
}
