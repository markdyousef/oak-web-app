{
  "app": {
    "name": "Test",
    "version": "0.0.1",
    "build": "1"
  },
  "private": true,
  "main": "main.js",
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src/; exit 0",
    "lint:css": "stylelint ./src/**/*.css",
    "test": "NODE_PATH=./src mocha --opts tools/tests/mocha.opts",
    "test:watch": "mocha --opts tools/tests/mocha.opts --watch",
    "build": "npm run build:prod",
    "build:dev": "NODE_ENV=development node -r babel-register tools/build.js",
    "build:prod": "NODE_ENV=production node -r babel-register tools/build.js",
    "clean": "rm -rf build && rm -rf release",
    "start": "npm run start:web:dev --",
    "start:web:dev": "NODE_ENV=development node -r babel-register tools/web/server",
    "start:web:prod": "NODE_ENV=production node -r babel-register tools/web/server",
    "start:app": "npm run start:app:dev --",
    "start:app:dev": "electron -r babel-register ./tools/electron/main.js",
    "start:server": "npm run start:server:dev --",
    "start:server:dev": "NODE_ENV=development node -r babel-register tools/electron/server.js --target app",
    "package": "NODE_ENV=production node -r babel-register tools/electron/package.js",
    "check": "npm outdated --depth=0"
  },
  "dependencies": {
    "fastclick": "1.0.6",
    "history": "2.1.2",
    "immutable": "3.8.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2015-webpack": "6.4.3",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.16.3",
    "babel-runtime": "6.11.6",
    "browser-sync": "2.17.3",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.2",
    "connect-history-api-fallback": "1.3.0",
    "css-loader": "0.25.0",
    "electron": "1.3.3",
    "electron-packager": "7.5.1",
    "enzyme": "2.4.1",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "express": "4.14.0",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "jsdom": "9.6.0",
    "json-loader": "0.5.4",
    "mocha": "3.1.2",
    "postcss-cssnext": "2.8.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "0.13.0",
    "postcss-reporter": "1.4.1",
    "react-addons-test-utils": "15.3.2",
    "react-hot-loader": "3.0.0-beta.5",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "stylelint": "7.4.2",
    "stylelint-config-standard": "13.0.2",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-target-electron-renderer": "0.4.0"
  }
}
